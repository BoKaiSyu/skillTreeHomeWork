@model IEnumerable<skillTreeMvcHomeWork.Models.ViewModels.MoneyData>

<table class="table">
    <tr>
        <th>
            #
        </th>
        <th>
            類別
        </th>
        <th>
            日期
        </th>
        <th>
            金額
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.moneyType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.money)
            </td>
        </tr>
    }

</table>
